{
  "name": "volley-team-management-service",
  "version": "1.0.0",
  "description": "Backend service for Volley ball team management",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "type-check": "tsc",
    "lint": "eslint --ext .ts src/**/*.ts --color",
    "lint:fix": "eslint --ext .ts src/**/*.ts --color --fix",
    "format": "prettier --write .",
    "knex:dev": "cross-env NODE_ENV=development knex",
    "knex:seed": "knex seed:run --knexfile src/knexfile.ts",
    "knex:migrate": "knex migrate:latest --knexfile src/knexfile.ts",
    "db:dev": "cross-env NODE_ENV=development npm run knex:migrate && npm run knex:seed",
    "build-js": "babel src --extensions \".ts\" -d dist",
    "build": "npm run clean && npm run lint && npm run type-check && npm run build-js",
    "dev:prestart": "npm run lint && npm run type-check",
    "dev": "cross-env NODE_ENV=development nodemon src/app.ts",
    "start": "cross-env NODE_ENV=production ISSUER=VTMSAuthService AUTH_TOKEN_EXPIRY=1h DB_USER=postgres DB_HOST=localhost DB_NAME=volley_team_mgmt_dev DB_PASSWORD= node dist/app.js"
  },
  "keywords": [
    "volley",
    "ball",
    "Team management",
    "Sports"
  ],
  "author": "Madhusudhanan",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "pg": "^8.7.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
